    <meta name="description" content="UpAgri">
    <meta name="keywords" content="application, plateforme, mobile, business, startup, technologie, innovation, agriculture, agroalimentaire, emulation,
incubation, upagri">
    <meta name="robots" content="index, follow">
    <meta name="revisit-after" content="1 month">
    <meta name=viewport content="width=device-width, initial-scale=1">

    <!-- FavIcon -->
    <link rel="icon" type="image/png" href="{{ asset('image/logo_upagri.png')}}" />


    {% block stylesheets %}

        <!-- Bootstrap -->
